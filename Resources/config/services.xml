<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="form.type.google_maps.class">CyberJaw\GoogleMapsBundle\Form\Type\GoogleMapsType</parameter>
    </parameters>

    <services>
        <!-- google_maps form type -->
        <service id="form.type.google_maps" class="%form.type.google_maps.class%">
            <tag name="form.type" alias="google_maps"/>
        </service>
    </services>
</container>
